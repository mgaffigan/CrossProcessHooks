<Window x:Class="Itp.Win32.DemoWpfTarget.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Itp.Win32.DemoWpfTarget"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBlock x:Name="lbTime" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Time:" VerticalAlignment="Top" />
        <TextBox x:Name="tbTime" Height="23" Margin="100,9,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" IsReadOnly="True" />
        <TextBlock x:Name="lbArb" HorizontalAlignment="Left" Margin="10,38,0,0" TextWrapping="Wrap" Text="Arbitrary:" VerticalAlignment="Top"/>
        <TextBox x:Name="tbArbitrary" Height="23" Margin="100,37,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>

    </Grid>
</Window>
